{
  "name": "target_myretail",
  "version": "1.0.1",
  "description": "Customer facing UI for the online retail grocery store",
  "repository": "https://github.com/visarts/target_myretail.git",
  "author": "brendanBest <brendan.best@target.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack && webpack-dev-server",
    "build": "webpack --config webpack.config.prod.js",
    "lint": "eslint /.eslintrc",
    "test": "jest --watch",
    "release": "standard-version"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-image-gallery": "^0.8.9",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.0",
    "jest-styled-components": "^5.0.1",
    "node-sass": "^4.9.2",
    "prop-types": "^15.6.2",
    "sass-loader": "^7.0.3",
    "standard-version": "^4.4.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/__tests__/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^common(.*)$": "<rootDir>/src/components/_common$1",
      "^config(.*)$": "<rootDir>/config$1",
      "^store(.*)$": "<rootDir>/src/store$1"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-console": 1
    },
    "globals": {
      "afterEach": true,
      "beforeEach": true,
      "describe": true,
      "expect": true,
      "it": true,
      "jest": true
    }
  }
}
